{
    "model": {
        "name": "vdsr"
    },
    "img": {
        "datasets": [
            {
                "name": "SCA2023",
                "subsets": [
                    "Images/Real_images/Faces"
                ],
                "limit": 1
            }
        ]
    },
    "distortion": [
        {
            "name": "refraction_datasets",
            "psf": {
                "datasets": [
                    {
                        "name": "SCA2023",
                        "subsets": [
                            "PSFs/Narrow"
                        ],
                        "limit": 1
                    }
                ]
            }
        }
    ],
    "loss_function": {
        "name": "MS_SSIM"
    },
    "batch_size": 4,
    "sample_size": 120,
    "optimizer": {
        "name": "Adam",
        "learning_rate": 0.01,
        "eps": 1e-8
    }
}