{
    "model": {
        "name": "precompensationusrnet",
        "path": "./olimp/weights/usrnet.pth"
    },
    "img": {
        "datasets": [
            {
                "name": "SCA2023",
                "subsets": [
                    "Images/Real_images/Faces"
                ]
            }
        ],
        "transforms": [
            {
                "name": "Grayscale",
                "num_output_channels": 3
            },
            {
                "name": "Resize",
                "size": [
                    512,
                    512
                ]
            }
        ]
    },
    "distortion": [
        {
            "name": "refraction_datasets",
            "psf": {
                "datasets": [
                    {
                        "name": "SCA2023",
                        "subsets": [
                            "PSFs/Narrow"
                        ]
                    }
                ]
            }
        }
    ],
    "batch_size": 1,
    "loss_function": {
        "name": "MS_SSIM"
    }
}